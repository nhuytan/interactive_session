<tool id='Matthew.Shaxted_interactive_session' name='Matthew.Shaxted_interactive_session'>
  <command interpreter='bash'>main.sh</command>
  <inputs>
    <param name='_pw_service_name' label='Service Name' type='hidden' value='nicedcv' width='50%_none'></param>
    <param name='_pw_service_port' label='Service Port' type='hidden' value='8443' width='50%_none'></param>
    <param name='_pw_service_display' label='Service Display' type='hidden' value='2' width='50%_none'></param>
    <param name='_pw_jobschedulertype' label='Service Scheduler Type' type='hidden' value='SLURM' width='50%_none'></param>
    <param name='_pw__sch__dd_partition_e_' label='Service Partition' type='hidden' value='vnc' width='50%_none'></param>
    <param name='_pw_scheduler_directives' label='Service Directives' type='hidden' value='--exclusive' width='50%_none'></param>
    <section name='service_sec' type='section' title='Paraview' expanded='false'>
        <param name='_pw_service_bin' label='Paraview launch command' type='text' value='__poolworkdir__/pw/bootstrap/ParaView-5.6.0-MPI-Linux-64bit/bin/paraview-----mesa' help='Use --- instead of spaces!' width='50%_none'></param>
   </section>
    <section name='advanced_options_other' type='section' title='Advanced Options' expanded='false'>
        <param name="_pw_stream" type="boolean" truevalue="Yes" falsevalue="No" checked="False" label="Stream slurm output?" help='Select Yes to stream the slurm output from the execution host to the job directory in PW' width="25%_none" optional='true' float="right">
        </param>
    </section>
  </inputs>
  <outputs>
  </outputs>
</tool>
